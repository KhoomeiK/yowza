{
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "editor.formatOnSave": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    // Eslint autofix on save
    "source.fixAll.eslint": true
  },
  "eslint.workingDirectories": [
    "web",
    "server"
  ]
}
