{
  "name": "yowza",
  "version": "1.0.0",
  "description": "Automatic generation of listicles from AskReddit threads",
  "main": "index.js",
  "scripts": {
    "_comment": "Start is for production, it requires to project to be built using npm:build",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start-react": "react-scripts start",
    "start-express": "node index.js",
    "start": "concurrently npm:start-react npm:start-express",
    "prod": "NODE_ENV=production node index.js"
  },
  "proxy": "http://localhost:8080",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhoomeiK/yowza.git"
  },
  "author": "Rohan Pandey & Miguel T",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KhoomeiK/yowza/issues"
  },
  "homepage": "https://github.com/KhoomeiK/yowza#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-infinite-scroller": "^1.2.4",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
